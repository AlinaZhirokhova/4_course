{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
      },
      "extends": ["eslint:recommended", "prettier"],
      "plugins": [
        "prettier"
      ],
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
      },
      "rules": {
        "prettier/prettier": "error",
        "camelcase": [
          "error"
        ],
        "eqeqeq": [
          "error",
          "always"
        ]
      }
    }